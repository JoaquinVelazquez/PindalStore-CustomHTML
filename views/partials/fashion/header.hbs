<div
  class="header-wrapper header-wrapper--fixed header-wrapper--with-transparency nav-logo-float-left  nav-logo-float-left{{headerAdditionalClasses}}"
  id="header-wrapper" data-ms-theme="fashion">
  {{#if flash.enabled}}
  {{> components/flash}}
  {{/if}}
  {{> components/cookieDisclaimer}}
  <header role="banner" class="nav-fashion-header" itemtype="http://schema.org/Store" itemid="#this" itemscope="">
    <!-- Desktop menu sidebar -->
    <div itemscope itemtype="http://schema.org/SiteNavigationElement"
      class="nav-fashion-menu nav-fashion-sidebar-menu header--custom-background" id="nav-vertical-list">
      <div class="close-sidebar-button">
        <input type="checkbox" id="nav-fashion__close-sidebar-button" name="nav-tools-header__menu-switch">
        <label for="nav-fashion__close-sidebar-button" id="close-sidebar-button">
          <span class="nav-header__close-sidebar-button nav-header__hamburger"></span>
          <span class="nav-header__close-sidebar-button nav-header__hamburger"></span>
        </label>
      </div>
      <ul class="sidebar-menu-list">
        {{#if sale}}
        <li class="nav-fashion-list--vertical__item nav-fashion__sale-link header--custom-border">
          <a href="{{sale.url}}" class="header--custom-text">{{sale.text}}</a>
        </li>
        <li class="nav-fashion-list__item nav-fashion__hamburger-button" id="nav-fashion__hamburger-button"></li>
        {{/if}}
        {{#each menu}}
        <li class="nav-fashion-list--vertical__item {{#if this.featured}}nav-fashion__sale-link{{/if}}">
          <a href="{{this.url}}" class="header--custom-text">{{this.name}}</a>
          {{#if this.nodes}}
          <div>
            <input type="checkbox" data-js="categories-toggle" id="desktop-{{@index}}"
              name="nav-tools-header__menu-switch">
            <label for="desktop-{{@index}}" class="toggle-menu">
              <span class="nav-header__toggle nav-header__hamburger"></span>
              <span class="nav-header__toggle nav-header__hamburger"></span>
            </label>
          </div>
          {{/if}}
        </li>
        <div class="nav-fashion-list--vertical__subitem-container" data-toggle="desktop-{{@index}}">
          {{#each this.nodes}}
          <li class="nav-fashion-list--vertical__subitem">
            <a href="{{this.url}}" class="header--custom-text">{{this.name}}</a>
            {{#if this.nodes}}
            <div>
              <input type="checkbox" data-js="categories-toggle" id="desktop-attr-{{@../index}}-{{@index}}"
                name="nav-tools-header__menu-switch">
              <label for="desktop-attr-{{@../index}}-{{@index}}" class="toggle-menu">
                <span class="nav-header__toggle nav-header__hamburger"></span>
                <span class="nav-header__toggle nav-header__hamburger"></span>
              </label>
            </div>
            {{/if}}
          </li>
          <div class="nav-fashion-list--vertical__attr-container" data-toggle="desktop-attr-{{@../index}}-{{@index}}">
            {{#each this.nodes}}
            <li class="nav-fashion-list--vertical__attribute">
              <a href="{{this.url}}" class="header--custom-text">{{this.name}}</a>
            </li>
            {{/each}}
          </div>
          {{/each}}
        </div>
        <hr class="nav-fashion-list--vertical__hr header--custom-border" />
        {{/each}}

        <li class="nav-fashion-list--vertical__item how-buy__button" data-js="how-buy__button">
          <a href="{{linkToHowBuy.href}}" class="header--custom-text">{{linkToHowBuy.label}}</a>
        </li>
      </ul>
    </div>

    <div class="nav-fashion-header__bounds">
      <!-- Desktop menu navbar categories -->
      <ul itemscope itemtype="http://schema.org/SiteNavigationElement" class="nav-fashion-list" id="nav-list">
        <li class="nav-fashion-list__item nav-fashion__hamburger-button" id="nav-fashion__hamburger-button">
